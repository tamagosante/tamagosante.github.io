{
  "domain": "lesswrong.com",
  "name": "LessWrong",
  "maxDepth": 10,

  "article": {
    "containerSelector": ".MultiToCLayout-content",
    "titleSelector": ".PostsPageTitle-root",
    "contentSelector": ".PostsPage-postContent"
